{
    "version": 2,
    "builds": [
      {
        "src": "nouegyan/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir":"staticfiles_build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "nouegyan/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
      
    ],
    "env": {
    "DJANGO_SETTINGS_MODULE": "nouegyan.settings",
